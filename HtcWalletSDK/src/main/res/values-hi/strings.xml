    <string name="confirm_your_pin_code_title">अपने पासकोड की पुष्टि करें</string><!--Confirm your passcode-->
    <string name="create_your_new_pin_code_title">अपना नया पासकोड बनाएँ</string><!--Create your new passcode-->
