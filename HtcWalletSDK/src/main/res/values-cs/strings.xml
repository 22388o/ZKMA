    <string name="recovery_key_introduction_title">Zazálohujte si svoji peněženku obnovovací větou o 12 slovech</string><!--Backup your wallet by 12-/word recovery phrase-->
    <string name="recovery_key_introduction_description">Tato fráze je jediný způsob obnovení vaší peněženky Zion Vault, pokud ztratíte telefon nebo zapomenete své heslo.</string><!--This set of words is the only way to recover your Zion Vault if you lose your phone or forget your passcode.-->
    <string name="recovery_key_introduction_button">Generovat obnovovací větu</string><!--Generate recovery phrase-->
    <string name="write_recovery_key_title">Zapište svoji obnovovací větu o 12 slovech ve správném pořadí</string><!--Write down your 12-/word recovery phrase in order-->
    <string name="show_recovery_key_title">Toto je vaše obnovovací věta o 12 slovech</string><!--This is your 12-/word recovery phrase-->
    <string name="write_recovery_key_description">Nezapomeňte si všech 12 slov zapsat ve správném pořadí a uschovejte je na bezpečném místě.</string><!--Make sure to write all 12 words down in the correct order and keep them in a safe place.-->
    <string name="write_recovery_key_button">Zapsal(a) jsem je</string><!--I\'ve written them down-->
    <string name="text_try_again">Jedno nebo více slov je nesprávných. Zkuste to znovu.</string><!--One or more words are incorrect. Try again.-->
    <string name="text_next_button">Další</string><!--Next-->
    <string name="text_recovery_verify_title">Ověřte svoji obnovovací větu(%1$d/3)</string><!--Verify your recovery phrase(%1$d/3)-->
    <string name="text_recovery_verify_description">Chcete-li ověřit svoji obnovovací větu o 12 slovech, zadejte 12 slov ve správném pořadí.</string><!--To verify your 12-/word recovery phrase, enter the 12 words in order.-->
    <string name="text_recovery_enter_title">Obnovte svoji peněženku Zion Vault(%1$d/3)</string><!--Recover your Zion Vault(%1$d/3)-->
    <string name="text_recovery_enter_description">Zadejte svoji obnovovací větu o 12 slovech ve správném pořadí.</string><!--Enter your 12-/word recovery phrase in order.-->

    <string name="text_recovery_duplicated_error">Neplatná fráze pro obnovu.</string><!--Invalid recovery phrase.-->

