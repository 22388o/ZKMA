    <string name="create_your_pin_code_title">Vytvorte svoj prístupový kód</string><!--Create your passcode-->
    <string name="confirm_your_pin_code_title">Potvrďte svoj prístupový kód</string><!--Confirm your passcode-->
    <string name="create_your_new_pin_code_title">Vytvorte svoj nový prístupový kód</string><!--Create your new passcode-->
    <string name="confirm_your_new_pin_code_title">Potvrďte svoj nový prístupový kód</string><!--Confirm your new passcode-->
    <string name="enter_pin_to_unlock_your_wallet_title">Ak chcete odomknúť svoju peňaženku, zadajte svoj prístupový kód</string><!--Enter your passcode to unlock your wallet-->
    <string name="enter_pin_to_continue_title">Pokračujte zadaním prístupového kódu</string><!--Enter your passcode to continue-->
    <string name="pin_open_app_desc">Tento prístupový kód bude potrebný na všetky transakcie a spustenie aplikácie.</string><!--This passcode will be required for all transactions and app startup.-->
    <string name="pin_unlock_wallet_desc">Tento prístupový kód bude potrebný na transakcie a odomknutie peňaženky.</string><!--This passcode will be required for transactions and wallet unlock.-->
    <string name="pin_codes_did_not_match">Prístupové kódy sa nezhodujú. Skúste to znova.</string><!--Your passcodes do not match. Try again.-->
    <string name="pin_codes_did_not_match2">Nesprávny prístupový kód. Skúste to znova.</string><!--Passcode incorrect. Please try again.-->
    <string name="confirm_remove_trusted_contact_title">Naozaj chcete odstrániť dôveryhodný kontakt?</string><!--Are you sure to remove this trusted contact?-->
    <string name="enter_pin_to_remove_desc">Ak chcete odstrániť, zadajte svoj prístupový kód</string><!--Enter your passcode to remove-->
    <string name="enter_pin_change_pin_title">Naozaj chcete zmeniť svoj prístupový kód?</string><!--Are you sure you want to change your passcode?-->
    <string name="enter_pin_change_subtitle">Ak chcete zmeniť, zadajte prístupový kód</string><!--Enter passcode to change-->
    <string name="enter_pin_show_recovery_key_title">Naozaj chcete zobraziť frázu na obnovenie?</string><!--Are you sure you want to show the recovery phrase?-->
    <string name="enter_pin_show_subtitle">Ak chcete zobraziť, zadajte prístupový kód</string><!--Enter your passcode to show-->
    <string name="enter_pin_reset_wallet_title">Naozaj chcete vynulovať svoju peňaženku?</string><!--Are you sure you want to reset your wallet?-->
    <string name="enter_pin_reset_wallet_subtitle">Ak chcete vynulovať, zadajte svoj prístupový kód</string><!--Enter your passcode to reset-->
    <string name="enter_pin_to_reset_wallet_title">Ak chcete vynulovať svoju peňaženku, zadajte svoj prístupový kód</string><!--Enter your passcode to reset your wallet-->
    <string name="enter_pin_to_reset_wallet_desc">Tým vymažete všetky údaje peňaženky. Na obnovenie svojej peňaženky budete potrebovať frázu na obnovenie.</string><!--This will wipe all wallet data. You will need your recovery phrase to recover your wallet.-->
    <string name="enter_pin_to_generate_verification_code_title">Zadajte svoj prístupový kód</string><!--Enter your passcode-->
    <string name="enter_pin_backup_security_check_title">Naozaj chcete použiť toto konto?</string><!--Are you sure to use this account?-->
    <string name="enter_pin_backup_security_check_desc">Vaše predchádzajúce údaje dôveryhodného kontaktu sa natrvalo stratia.</string><!--Your previous trusted contact data will be permanently lost.-->
    <string name="enter_pin_log_out_title">Naozaj sa chcete odhlásiť?</string><!--Are you sure you want to sign out?-->
    <string name="enter_pin_log_out_desc">Všetky údaje dôveryhodného kontaktu sa stratia.</string><!--All trusted contact data will be lost.-->
    <string name="enter_pin_log_out_subtitle">Ak sa chcete odhlásiť, zadajte prístupový kód</string><!--Enter your passcode to sign out-->

