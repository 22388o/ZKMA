    <string name="recovery_key_introduction_title">Varundage oma rahakott 12-sõnalise taastefraasi abil</string><!--Backup your wallet by 12-/word recovery phrase-->
    <string name="recovery_key_introduction_description">12-sõnaline taastefraas on ainus võimalus rahakoti taastamiseks, kui kaotate telefoni või unustate tehingute pääsukoodi.</string><!--The 12-/word recovery phrase is the only way to restore your wallet in case you lose your phone or forget your transaction passcode.-->
    <string name="recovery_key_introduction_button">Taastefraasi loomine</string><!--Generate recovery phrase-->
    <string name="write_recovery_key_title">Kirjutage 12 sõnast koosnev taastefraas õiges järjestuses üles</string><!--Write down your 12-/word recovery phrase in order-->
    <string name="show_recovery_key_title">See on teie 12-sõnaline taastefraas</string><!--This is your 12-/word recovery phrase-->
    <string name="write_recovery_key_description">Pange kõik 12 sõna õiges järjestuses kirja ja hoidke neid turvalises kohas.</string><!--Make sure to write all 12 words down in the correct order and keep them in a safe place.-->
    <string name="write_recovery_key_button">Olen need kirja pannud</string><!--I\'ve written them down-->
    <string name="text_try_again">Üks või mitu sõna on valed. Pr. uuesti.</string><!--One or more words are incorrect. Try again.-->
    <string name="text_next_button">Järgmine</string><!--Next-->
    <string name="text_recovery_verify_title">Kinnitage oma taastefraas (%1$d/3)</string><!--Verify your recovery phrase(%1$d/3)-->
    <string name="text_recovery_verify_description">12-sõnalise taastefraasi kinnitamiseks sisestage 12 sõna õiges järjestuses.</string><!--To verify your 12-/word recovery phrase, enter the 12 words in order.-->
    <string name="text_recovery_enter_title">Rahakoti taastamine (%1$d/3)</string><!--Recover your wallet(%1$d/3)-->
    <string name="text_recovery_enter_description">Sisestage 12-sõnaline taastefraas õiges järjestuses.</string><!--Enter your 12-/word recovery phrase in order.-->

